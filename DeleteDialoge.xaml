<Window x:Class="GymManagementSystem.DeleteDialoge"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Delete Client" Height="50" Width="200"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Content="Delete" Width="60" Margin="5" Click="DeleteButton_Click"/>
            <Button Content="Cancel" Width="60" Margin="5" Click="CancelButton_Click"/>
        </StackPanel>
    </Grid>
</Window>